<Window x:Class="WpfApp1.View.Dialogs.BranchDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.View.Dialogs"
        mc:Ignorable="d"
        Title="BranchDialog" Height="150" Width="300">

    <StackPanel Orientation="Horizontal" Height="30">
        <TextBox Width="150" Margin="10,5" Name="NameTextbox"/>
        <Button Content="OK" Margin="5,0" Width="50" Name="OkBtn" Click="OkBtn_Click"/>
        <Button Content="Cancel" Margin="5,0" Width="50" Name="CancleBtn" Click="CancleBtn_Click"/>
    </StackPanel>

</Window>
