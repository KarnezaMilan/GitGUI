<Window x:Class="WpfApp1.View.Dialogs.CommitDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.View.Dialogs"
        mc:Ignorable="d"
        Height="200" Width="300" WindowStartupLocation="CenterScreen" >




    <Grid Background="Black" >
        <StackPanel Style="{StaticResource BackGroundStyle}" Margin="2">
            <StackPanel Margin="25,20,25,15">
                <Label Content="Name: " Padding="0"/>
                <TextBox Name="UserNameTextbox"/>
                <Label Content="Email: " Padding="0"/>
                <TextBox Name="EmailTextbox" />

            </StackPanel>
            <StackPanel Margin="25,0">
                <Button Content="Conform" Margin="0,5" Name="ConformBtn" Click="ConformBtn_Click"/>
            </StackPanel>

        </StackPanel>

    </Grid>
</Window>
